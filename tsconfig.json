{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@data": ["src/data/data.json"],
      "@layouts/*": ["src/layouts/*"],
      "@scripts/*": ["src/scripts/*"]
    }
  }
}
